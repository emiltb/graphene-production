@Article{Akbari2016,
  author   = {Akbari, Abozar and Sheath, Phillip and Martin, Samuel T. and Shinde, Dhanraj B. and Shaibani, Mahdokht and Banerjee, Parama Chakraborty and Tkacz, Rachel and Bhattacharyya, Dibakar and Majumder, Mainak},
  title    = {Large-area graphene-based nanofiltration membranes by shear alignment of discotic nematic liquid crystals of graphene oxide},
  journal  = {Nat Commun},
  year     = {2016},
  volume   = {7},
  abstract = {Graphene-based membranes demonstrating ultrafast water transport, precise molecular sieving of gas and solvated molecules shows great promise as novel separation platforms; however, scale-up of these membranes to large-areas remains an unresolved problem. Here we demonstrate that the discotic nematic phase of graphene oxide (GO) can be shear aligned to form highly ordered, continuous, thin films of multi-layered GO on a support membrane by an industrially adaptable method to produce large-area membranes (13 [times] 14[thinsp]cm2) in <5[thinsp]s. Pressure driven transport data demonstrate high retention (>90%) for charged and uncharged organic probe molecules with a hydrated radius above 5[thinsp]A as well as modest (30-40%) retention of monovalent and divalent salts. The highly ordered graphene sheets in the plane of the membrane make organized channels and enhance the permeability (71[plusmn]5[thinsp]l[thinsp]m-2[thinsp]hr-1[thinsp]bar-1 for 150[plusmn]15[thinsp]nm thick membranes).},
  comment  = {Supplementary information available for this article at http://www.nature.com/ncomms/2016/160307/ncomms10891/suppinfo/ncomms10891_S1.html},
  refid    = {2},
  url      = {http://dx.doi.org/10.1038/ncomms10891},
}

@Article{Allongue1997,
  author   = {Allongue, Philippe and Delamar, Michel and Desbat, Bernard and Fagebaume, Olivier and Hitmi, Rachid and Pinson, Jean and Savéant, Jean-Michel},
  title    = {Covalent Modification of Carbon Surfaces by Aryl Radicals Generated from the Electrochemical Reduction of Diazonium Salts},
  journal  = {Journal of the American Chemical Society},
  year     = {1997},
  volume   = {119},
  number   = {1},
  pages    = {201-207},
  abstract = {Electrochemical reduction of a wide variety of aromatic diazonium salts on carbon electrodes (glassy carbon, highly oriented pyrolytic graphite) leads to the covalent attachment of the corresponding aromatic radicals. The films thus deposited on glassy carbon surfaces require mechanical abrasion to be removed. Cyclic voltammetry, X-ray photoelectron spectroscopy, polarization modulation IR reflection absorption spectroscopy, Auger spectroscopy, and Rutherford backscattering spectroscopy allow the characterization of the overlayer and an estimate of the surface coverage. The latter can be controlled through diazonium concentration and electrolysis duration. The mechanism of derivatization is discussed on the basis of the kinetic data obtained from cyclic voltammetry and preparative electrolysis. This versatile method of surface modification may find applications in the field of carbon−epoxy composites as attested by the successful binding of grafted p-aminophenyl groups with epichlorhydrin.},
  refid    = {3},
  url      = {http://dx.doi.org/10.1021/ja963354s},
}

