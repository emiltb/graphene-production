  journal  = {Nat Commun},
  year     = {2015},
  volume   = {6},
  abstract = {The basal plane cleavage energy (CE) of graphite is a key material parameter for understanding many of the unusual properties of graphite, graphene and carbon nanotubes. Nonetheless, a wide range of values for the CE has been reported and no consensus has yet emerged. Here we report the first direct, accurate experimental measurement of the CE of graphite using a novel method based on the self-retraction phenomenon in graphite. The measured value, 0.37[plusmn]0.01[thinsp]J[thinsp]m-2 for the incommensurate state of bicrystal graphite, is nearly invariant with respect to temperature (22[thinsp][deg]C[le]T[le]198[thinsp][deg]C) and bicrystal twist angle, and insensitive to impurities from the atmosphere. The CE for the ideal ABAB graphite stacking, 0.39[plusmn]0.02[thinsp]J[thinsp]m-2, is calculated based on a combination of the measured CE and a theoretical calculation. These experimental measurements are also ideal for use in evaluating the efficacy of competing theoretical approaches.},
  comment  = {Supplementary information available for this article at http://www.nature.com/ncomms/2015/150828/ncomms8853/suppinfo/ncomms8853_S1.html},
  refid    = {66},
  url      = {http://dx.doi.org/10.1038/ncomms8853},
}

@Article{Wang2011,
