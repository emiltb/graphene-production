@Article{Ishikawa2012,
  author   = {Ishikawa, R. and Ko, P. J. and Kurokawa, Y. and Konagai, M. and Sandhu, A.},
  title    = {Electrophoretic deposition of high quality transparent conductive graphene films on insulating glass substrates},
